.switch-hot:after {
    content: "";
    display: block;
    position: absolute;
    bottom: 2rpx;
    width: 40rpx;
    width: 5.33333vw;
    border-radius: 2rpx;
    height: 6rpx;
    background-color: #f03d37;
    -webkit-transition: left .2s;
    transition: left .2s;
}
.switch-hot .hot-item.active {
    color: #333;
    font-weight: 700;
}
.switch-hot[data-active=".n-hot"]:after {
	left:32rpx;
}
.switch-hot[data-active=".cinema"]:after {
	left:136rpx;
}
.switch-hot[data-active=".f-hot"]:after {
	left:240rpx;
}
.switch-hot[data-active=".classic"]:after {
	left:376rpx;
}
